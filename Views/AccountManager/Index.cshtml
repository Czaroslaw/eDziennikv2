@model List<ApplicationUser>
@{
    ViewData["Title"] = "Zarządzaj użytkownikami";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Zarządzaj użytkownikami</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                Imię
            </th>
            <th>
                Nazwisko
            </th>
            <th>
                Zarządzaj
            </th>
        </tr>
    </thead>
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>
                @Model.ToArray()[i].FirstName
            </td>
            <td>
                @Model.ToArray()[i].LastName
            </td>
            <td>
                <a asp-action="Edit" asp-controller="AccountManager" asp-route-id="@Model.ToArray()[i].Id">Zarządzaj</a>
            </td>
        </tr>
    }
</table>
</table>